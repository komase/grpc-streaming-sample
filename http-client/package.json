  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "axios": "^1.6.5",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "prettier": "3.1.1",
    "typescript": "^5.3.3"
  }
}
